<h1>Wikis</h1>
<div class="container">
  <div class="row">
    <div class="col">
      <button type="button" class="btn btn-success"><%= link_to "New Wiki", new_wiki_path(@wiki) %> </button>
    </div>
  </div>
</div>

<% @wikis.each do |wiki| %>
  <div class="media">
  <div class="media-body">
    <h4 class="media-heading">
      <%= link_to wiki.title, wiki %>
    </h4>
  </div>
  </div>
<% end %>
